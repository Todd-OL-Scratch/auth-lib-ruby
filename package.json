  "scripts": {
    "dev": "cross-env BUILD_ENV=qa node server.js",
    "local-dev": "cross-env BUILD_ENV=development node server.js",
    "local-qa": "cross-env BUILD_ENV=qa node server.js",
    "local-prod": "cross-env BUILD_ENV=production node server.js",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=production next build",
    "bundle-analizer": "cross-env BUILD_ENV=analyze next build",
    "start": "cross-env NODE_ENV=production node server.js",
    "test": "jest -u --coverage && coveralls < coverage/lcov.info",
    "test:ci": "jest -w=2 --ci -u",
    "test:coverage": "yarn run test:ci -- --coverage && coveralls < coverage/lcov.info",
    "lint": "eslint .",
    "check-formatting": "prettier --list-different '{src,pages}/**/*.js'",
    "fix-formatting": "prettier --list-different --write '{src,pages}/**/*.js'",
    "fix": "eslint . --fix",
    "scan-translations": "cross-env APP_NAMESPACE=contacts kw-scan --config ./node_modules/kw-i18next-scanner/lib/cjs/i18next.scanner.config.js 'pages/**/*.{js,jsx}' 'src/**/*.{jsx,js}'",
    "duplicates": "inspectpack -s stats.json -a \"duplicates\"",
    "duplicates:json": "inspectpack -s stats.json -a \"duplicates\" -f json",
    "checkResolutions": "node ./checkResolutions.js"
  },
