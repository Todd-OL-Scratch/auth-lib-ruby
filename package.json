    "jest-circus": "27.x",
    "jest-css-modules-transform": "^4.0.0",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^8.1.5",
    "lodash-webpack-plugin": "^0.11.5",
    "next-compose-plugins": "^2.1.1",
    "next-plugin-transpile-modules": "^1.0.0",
    "prettier": "^1.15.3",
    "sass-resources-loader": "^2.0.0",
    "string-replace-loader": "^3.1.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-stats-plugin": "^1.0.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && yarn checkResolutions"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json}": [
        "prettier --write --config .prettierrc",
        "git add"
      ],
      "*.js": "eslint",
      "*.scss": [
        "csscomb --tty-mode --",
        "git add"
      ]
    },
    "ignore": [
      "**/.next/**/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testRunner": "jest-circus/runner",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|sass|scss)$": "<rootDir>/node_modules/jest-css-modules-transform"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!kw-utils|ui-console-shell).+\\.js$"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "pages/**/*.{js,jsx}"
    ],
    "testEnvironment": "jsdom"
  }
}
