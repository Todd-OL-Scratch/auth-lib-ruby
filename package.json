    "eslint-config-kwri": "KWRI/eslint-config-kwri",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-babel-module": "^5.3.1",
